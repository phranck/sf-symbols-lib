{
  "name": "sf-symbols-lib",
  "version": "1.0.0",
  "description": "SF Symbols React components library",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./hierarchical": {
      "types": "./dist/hierarchical/index.d.ts",
      "import": "./dist/hierarchical/index.js"
    },
    "./monochrome": {
      "types": "./dist/monochrome/index.d.ts",
      "import": "./dist/monochrome/index.js"
    },
    "./multicolor": {
      "types": "./dist/multicolor/index.d.ts",
      "import": "./dist/multicolor/index.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "generate": "tsx scripts/generate-sfsymbols.ts",
    "minify:dist": "node scripts/minify-dist.js",
    "generate:prod": "npm run generate && npm run minify:dist",
    "lint": "eslint .",
    "clean:generated": "rm -rf src/hierarchical src/monochrome src/multicolor src/components/sf-symbol-name.ts src/index.ts docs/dist",
    "page": "npm run generate && browser-sync start --server docs/dist --files docs/dist/index.html --no-open",
    "page:only": "browser-sync start --server docs/dist --files docs/dist/index.html --no-open",
    "check": "npm run lint && npm run typecheck && npm run build"
  },
  "dependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.6",
    "@vitejs/plugin-react": "^5.1.2",
    "browser-sync": "^3.0.4",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.0.0",
    "terser": "^5.20.0"
  }
}
